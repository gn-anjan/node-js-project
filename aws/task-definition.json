{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "node-project-new",
    "containerDefinitions": [
        {
            "name": "node-project",
            "image": "182622655008.dkr.ecr.us-east-1.amazonaws.com/node-pro:#APP_VERSION#",
            "portMappings": [{
                "name": "node-tcp",
                "containerPort": 3000,
                "hostPort": 3000,
                "protocol": "tcp"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::182622655008:role/ecsTaskExecutionRole"
}