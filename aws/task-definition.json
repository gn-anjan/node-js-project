{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "node-project-new",
    "containerDefinitions": [
        {
            "name": "nginx-pod",
            "image": "nginx:latest",
            "portMappings": [{
                "name": "nginx-tcp",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": ""
}